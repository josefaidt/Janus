{
  "name": "discord-hansl",
  "version": "0.0.5",
  "description": "My Node.js Discord bot",
  "main": "bot.js",
  "author": "josef",
  "scripts": {
    "start": "node src/bot.js",
    "build": "rimraf dist && babel src -d dist && mkdir dist/etc; cp src/etc/config.json dist/etc/",
    "check": "standard \"src/**/*.js\"",
    "fix": "standard --fix \"src/**/*.js\"",
    "client": "webpack-dev-server --mode development --hot"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "dependencies": {
    "discord.js": "^11.3.2",
    "gdax": "*",
    "require-directory": "^2.1.1",
    "weather-js": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "rimraf": "^2.6.2",
    "standard": "^10.0.3"
  },
  "engines": {
    "node": "10"
  },
  "repository": "https://github.com/josefaidt/discord-hansl.git",
  "license": "MIT"
}
